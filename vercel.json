{
    "version": 2,
    "builds": [
      { "src": "src/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/", "dest": "/src/index.html" },
      { "src": "/homepage", "dest": "/src/homepage.html" },
      { "src": "/api", "dest": "/src/api.html" },
      { "src": "/asset/(.*)", "dest": "/src/asset/$1" },
      { "src": "/(.*)\\.js", "dest": "/src/$1.js" },
      { "src": "/(.*)\\.css", "dest": "/src/$1.css" },
      { "handle": "filesystem" }
    ]
  }